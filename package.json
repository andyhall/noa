    "gl-vec3": "^1.0.3",
    "ndarray": "^1.0.16",
    "ndarray-hash": "^1.0.0",
    "voxel-aabb-sweep": "^0.5.0",
    "voxel-physics-engine": "^0.9.0"
  },
  "repository": "https://github.com/andyhall/noa",
  "bugs": {
